{
  "name": "@vardario/zod-form-validation",
  "version": "0.1.12",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "browser": "dist/browser.js",
  "scripts": {
    "build": "pnpm build:lib && pnpm build:browser",
    "build:lib": "tsc --build",
    "build:browser": "esbuild src/index.ts --bundle --outfile=dist/browser.js --platform=neutral --external:zod",
    "watch": "concurrently \"pnpm watch:lib\" \"pnpm watch:browser\"",
    "watch:lib": "pnpm build:lib --watch",
    "watch:browser": "pnpm build:browser --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [],
  "author": "Sahin Vardar",
  "license": "MIT",
  "devDependencies": {
    "@types/jsdom": "^21.1.4",
    "@types/lodash": "^4.14.199",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.5",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
    "serve": "^14.2.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "zod": "^3.22.4"
  },
  "packageManager": "pnpm@8.6.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:vardario/zod-form-validation.git"
  }
}
